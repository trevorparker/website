<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <url>
    <loc>https://www.trevorparker.com/</loc>
    <lastmod>{{ site.time | date: "%Y-%m-%d" }}</lastmod>
    <changefreq>daily</changefreq>
  </url>
  <url>
    <loc>https://www.trevorparker.com/resume/</loc>
    <lastmod>2014-02-04</lastmod>
    <changefreq>monthly</changefreq>
  </url>
  <url>
    <loc>https://www.trevorparker.com/frankenstorm/</loc>
    <lastmod>2012-10-31</lastmod>
    <changefreq>never</changefreq>
  </url>
  {% for post in site.posts %}
  <url>
    <loc>https://www.trevorparker.com{{ post.url }}/</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>monthly</changefreq>
  </url>
  {% endfor %}
</urlset>
